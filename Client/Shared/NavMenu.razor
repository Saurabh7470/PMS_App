    <MudPaper Class="pa-2" Square Outlined>
        <MudNavMenu Color="Color.Warning" Rounded=true Class="pa-2">
            <MudNavLink Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home" Href="/">Dashboard</MudNavLink>
            <MudNavLink Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Work" Href="ProjectDetails">Projects</MudNavLink>
            <MudNavLink Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.WorkspacePremium" Href="AssignedProject">Assigne Projects</MudNavLink>
            <MudNavLink Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.InsertDriveFile" Href="ProjectReport">Report</MudNavLink>
        </MudNavMenu>
    </MudPaper>

</MudStack>

